{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": 0,
    "import/no-extraneous-dependencies": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "class-methods-use-this": "off"
  }
}
